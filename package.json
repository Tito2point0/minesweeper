{
  "name": "minesweeper",
  "version": "1.0.0",
  "description": "Minesweeper JavaScript clone to help strengthen front-end and back-end coding expertise.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "fkill :9000 :3000 -s && concurrently \"npm:backend\" \"npm:frontend\"",
    "frontend": "webpack serve --open",
    "backend": "node index.js",
    "start": "npm run backend",
    "server": "nodemon index.js",
    "webpack": "process.env.NODE_ENV=production webpack",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "reset": "npm run rollback && npm run migrate && npm run seed",
    "seed": "knex seed:run",
    "initialmig": "npx knex migrate:make initial-migration",
    "cleanup": "knex seed:run --specific=01-cleanup.js",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "migup": "knex migrate:up",
    "migdown": "knex migrate:down",
    "stopserver": "npx kill-port 5000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dilemmaemma/minesweeper.git"
  },
  "author": "dilemmaemma and tito",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dilemmaemma/minesweeper/issues"
  },
  "homepage": "https://github.com/dilemmaemma/minesweeper#readme",
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/plugin-transform-react-jsx": "7.17.12",
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "babel-loader": "8.2.5",
    "babel-plugin-styled-components": "2.0.7",
    "concurrently": "7.2.1",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "eslint": "8.17.0",
    "eslint-plugin-react": "7.30.0",
    "fkill-cli": "7.1.0",
    "html-loader": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "msw": "0.42.0",
    "nodemon": "2.0.16",
    "string-replace-loader": "3.1.0",
    "style-loader": "3.3.1",
    "webpack": "^5.73.0",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "axios": "0.27.2",
    "cors": "2.8.5",
    "db": "^4.0.1",
    "dotenv": "^16.3.1",
    "env": "^0.0.2",
    "express": "4.18.1",
    "knex": "^2.5.1",
    "knex-cleaner": "^1.3.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0",
    "request-ip": "^3.3.0",
    "sqlite3": "^5.1.6",
    "styled-components": "5.3.5",
    "webpack-cli": "^5.1.4",
    "yup": "0.32.11"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  }
}
